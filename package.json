{
    "name": "swipeforfuture",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "gh-pages": "^2.1.1",
        "react": "^16.10.2",
        "react-dom": "^16.10.2",
        "react-icons": "^3.7.0",
        "react-scripts": "3.2.0",
        "react-spring": "^7.2.8",
        "react-with-gesture": "^4.0.2",
        "styled-components": "^4.4.0"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "predeploy": "yarn build",
        "deploy": "gh-pages -d build"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "husky": "^3.1.0",
        "prettier": "^1.19.1",
        "pretty-quick": "^2.0.1",
        "styled-components.macro": "^1.0.0"
    },
    "homepage": "https://swipeforfuture.com",
    "husky": {
        "hooks": {
            "pre-commit": "pretty-quick --staged"
        }
    },
    "prettier": {
        "tabWidth": 4,
        "semi": false,
        "singleQuote": true,
        "trailingComma": "all"
    }
}
